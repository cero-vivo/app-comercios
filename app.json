{
  "expo": {
    "name": "5asec",
    "slug": "app-comercios-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1.png",
    "splash": {
      "image": "./assets/images/splash-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B038A"
    },
    "backgroundColor": "#3B038A",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.corazonesestudio.AppComercios",
      "config": {
        "googleMapsApiKey": ""
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-1.png",
        "backgroundColor": "#3B038A"
      },
      "package": "com.corazonesestudio.AppComercios",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-maps",
        {
          "requestLocationPermission": "true",
          "locationPermission": "Allow AppComercios to use your location"
        }
      ],
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#3B038A",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#3B038A"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "231a12f4-27c0-456d-8189-31cae738c0cc"
      }
    },
    "owner": "cero.vivo"
  }
}
