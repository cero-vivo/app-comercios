{
  "expo": {
    "name": "5asec",
    "slug": "AppComercios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1.png",
    "splash": {
      "image": "./assets/images/splash-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B038A"
    },
    "backgroundColor": "#3B038A",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.corazonesestudio.AppComercios",
      "config": {
        "googleMapsApiKey": "AIzaSyB4FG-2d3SRkWcgJ0FxedfgEmh9E8Mr9JY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-1.png",
        "backgroundColor": "#3B038A"
      },
      "package": "com.corazonesestudio.AppComercios",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB4FG-2d3SRkWcgJ0FxedfgEmh9E8Mr9JY"
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-maps",
        {
          "requestLocationPermission": "true",
          "locationPermission": "Allow AppComercios to use your location"
        }
      ],
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#3B038A",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#3B038A"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}